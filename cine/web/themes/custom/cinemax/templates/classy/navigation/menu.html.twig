{#
/**
 * @file
 * Theme override to display a menu.
 * Available variables:
 */
#}

{% import _self as menus %}
{{ menus.menu_links(items, attributes, 0) }}

{% macro menu_links(items) %}
    {% if items %}
      <ul>
      {% for item in items %}
        <li>
          {{ link(item.title, item.url) }}
        </li>
      {% endfor %}
      </ul>
    {% endif %}
{% endmacro %}